{
  "private": true,
  "scripts": {
    "start": "cross-env ELEVENTY_ENV=development npm-run-all clean dev:eleventy",
    "build": "cross-env ELEVENTY_ENV=production npm-run-all clean build:eleventy",
    "clean": "rimraf build",
    "dev:eleventy": "eleventy --serve --quiet",
    "build:eleventy": "eleventy",
    "test": "npm-run-all editorconfig html css javascript",
    "editorconfig": "editorconfig-checker",
    "html": "java -jar node_modules/vnu-jar/build/dist/vnu.jar --filterfile .vnurc build/**/*.html",
    "css": "stylelint \"src/**/*.scss\"",
    "javascript": "eslint eleventy.config.js src/**/*.js"
  },
  "engines": {
    "node": "19"
  },
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "editorconfig-checker": "^5.0.1",
    "eleventy-plugin-icons": "^2.0.0-4",
    "eleventy-sass": "^2.2.1",
    "esbuild": "^0.17.15",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "html-prettify": "^1.0.7",
    "i": "^0.3.7",
    "js-yaml": "^4.1.0",
    "npm": "^9.6.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-csso": "^6.0.1",
    "postcss-media-minmax": "^5.0.0",
    "rimraf": "^4.4.1",
    "stylelint": "^15.4.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-order": "^6.0.3",
    "vnu-jar": "^23.4.11"
  },
  "dependencies": {
    "gsap": "^3.11.5"
  }
}
